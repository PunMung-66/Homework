{
    "builds": [
        {
            "src": "public/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "src/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "scripts/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "asset/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ],
    "outputDirectory": "public"
}
